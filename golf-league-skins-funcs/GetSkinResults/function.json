{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "{league}/skins/{roundYear:int}/{roundNumber:int}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "skinResultsDocument",
      "databaseName": "golfleague",
      "collectionName": "golfleague-skinresults",
      "connectionStringSetting": "CosmosDbConnectionString",
      "id": "{roundYear}{roundNumber}",
      "partitionKey": "{league}"
    }
  ]
}
