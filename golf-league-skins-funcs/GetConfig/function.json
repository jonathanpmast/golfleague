{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "{league}/config"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "configData",
      "databaseName": "golfleague",
      "collectionName": "golfleague-config",
      "connectionStringSetting": "CosmosDbConnectionString",      
      "partitionKey": "{league}"
    }
  ]
}
