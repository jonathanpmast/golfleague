{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "scores"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "score",
      "databaseName": "golfleague",
      "collectionName": "golfleague-scores",
      "createIfNotExists": "true",
      "connectionStringSetting": "CosmosDbConnectionString"
    }
  ]
}
