{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "{league}/scores/{date:long}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "score",
      "databaseName": "golfleague",
      "collectionName": "golfleague-scores",
      "connectionStringSetting": "CosmosDbConnectionString",
      "id": "{roundId}",
      "partitionKey": "{league}",
      "sqlQuery": "select * from \"golfleague-scores\" as c where c.roundId={date}"
    }
  ]
}
