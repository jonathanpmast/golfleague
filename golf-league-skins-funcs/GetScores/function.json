{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "{league}/scores/{roundYear:int}/{roundNumber:int}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "scoreDoc",
      "databaseName": "golfleague",
      "collectionName": "golfleague-scores",
      "connectionStringSetting": "CosmosDbConnectionString",      
      "partitionKey": "{league}",
      "id": "{roundYear}{roundNumber}"
    }
  ]
}
